@inject MudBlazor.ISnackbar snackBar

<MudDialog @bind-IsVisible="visible" Options="dialogOptions"
           ConfirmedChanged="Test">
    <TitleContent>
        <MudText Typo="Typo.h6">
            Delete Tenant
        </MudText>
    </TitleContent>
    <DialogContent>
        <p>Are you sure, Delete selected tanant?</p>
    </DialogContent>
    <DialogActions>
        <MudButton Variant="Variant.Filled" OnClick="Cancel">Cancel</MudButton>
        <MudButton Variant="Variant.Filled" Color="Color.Primary" OnClick="@(async ()=> await Delete(DeleteId))" Class="px-10">Delete</MudButton>
    </DialogActions>
</MudDialog>